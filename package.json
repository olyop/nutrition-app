	"scripts": {
		"prestart": "kill-port 3000 3001",
		"start": "npm-run-all init build:client-typescript build:client-babel -p dev:*",
		"build-host": "npm-run-all build host",
		"build": "npm-run-all init build:*",
		"prehost": "kill-port 80",
		"host": "node -r dotenv/config build/server/index.js",
		"build-docker": "npm-run-all build docker:build",
		"build-docker-host": "npm-run-all build-docker docker:host",
		"build-docker-push": "npm-run-all build-docker docker:push",
		"dev:client-babel": "babel src/server -D --quiet -w -d build/server",
		"dev:client-nodemon": "nodemon -r dotenv/config build/server",
		"dev:client-webpack": "cross-env TS_NODE_PROJECT=\"tsconfig.webpack.json\" NODE_OPTIONS=\"-r dotenv/config\" webpack serve",
		"dev:client-typescript": "tsc --watch --project tsconfig.server.json",
		"build:client-babel": "babel -d build/server -D --quiet src/server",
		"build:client-webpack": "cross-env TS_NODE_PROJECT=\"tsconfig.webpack.json\" NODE_OPTIONS=\"-r dotenv/config\" webpack",
		"build:client-typescript": "tsc --project tsconfig.server.json",
		"build:tailwind": "npx tailwindcss-cli@latest build --no-autoprefixer -o src/client/tailwind.scss",
		"docker:build": "docker build -t olyop/nutrition-app:server-latest .",
		"docker:push": "docker push olyop/nutrition-app:server-latest",
		"docker:host": "docker run -p 80:80 olyop/nutrition-app:server-latest",
		"init": "clear && del-cli build",
		"sync": "git checkout . && git remote update && git pull",
		"push": "git commit -a -m \"Update\" && git push -u origin master",
		"publish-common": "tsc --outDir src/common --project tsconfig.common.json && cd src/common && npm publish && cd ../.. && npm i -S -P @oly_op/nutrition-app-common@latest",
		"test": "clear && jest --setupFiles dotenv/config",
		"postinstall": "del node_modules/@types/webpack"
	},
